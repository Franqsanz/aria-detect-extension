{
  "manifest_version": 2,
  "name": "ARIA Detect",
  "description": "Detecta si est√° aplicado 'ARIA-label' en las etiquetas HTML.",
  "short_name": "ARIA Detect",
  "version": "0.0.1",
  "icons": {
    "128": "./icons/icon128.png",
    "48": "./icons/icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"],
      "css": ["detect.css"]
    }
  ],
  "browser_action": {
    "default_title": "ARIA Detect",
    "default_icon": "./icons/icon128.png"
  }
}
